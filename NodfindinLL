public class Solution {
	public static int findNode(LinkedListNode<Integer> head, int n) {
		LinkedListNode<Integer> temp = head;
        int index=0;
        while(temp!=null)
        {
            if(temp.data==n)
            {
                return index;
            }
            temp=temp.next;
            index++;
        }
        return -1;
	}
}
